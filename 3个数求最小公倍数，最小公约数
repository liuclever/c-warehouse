1.#include<stdio.h>

max(int a, int b)
{
	if (a > b)
		return a;
}

main()
{
	int a, b, c,d,e,i;
	printf("请输入3个数：\n");
	scanf_s("%d%d%d", &a, &b, &c);
	d = max(a, b);
	e =  max(d, c);
	for (i = e; i >=1; i--)
	{
		if (a % i == 0 && b % i == 0 && c % i == 0)
		{
			printf("\n最大公约数为：%d", i);
			break;
		}
	}
	
}

while (i)
	{
		f = i * e;
		if (f%a == 0 && f%b == 0 && f%c== 0)
		{
			printf("\n最大公约数为：%d", f);
			break;
		}
		i++;

    }
